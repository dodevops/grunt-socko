    "gruntplugin",
    "grunt plugin",
    "grunt",
    "socko",
    "hierarchy",
    "configuration management"
  ],
  "author": "Dennis Ploeger <dennis.ploeger@kps.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:dodevops/grunt-socko.git"
  },
