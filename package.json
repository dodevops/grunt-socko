  "devDependencies": {
    "@types/bluebird": "^3.5.19",
    "@types/del": "^3.0.0",
    "@types/fs-extra": "^5.0.1",
    "@types/grunt": "^0.4.22",
    "@types/loglevel": "^1.5.3",
    "@types/node": "^9.4.7",
    "baseman": "^0.1.0-dev.8",
